{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,

  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],

  "rewrites": [
    {
      "source": "/:locale(en|id|zh)/images/:path*",
      "destination": "/images/:path*"
    }
  ]
}
